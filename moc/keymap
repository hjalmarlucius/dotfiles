# Key bindings for directories: defaults are Shift-number
# shift 1 = ! = Fastdir1
go_to_fast_dir1 = !
go_to_fast_dir2 = @
go_to_fast_dir3 = #
go_to_fast_dir4 = $
go_to_fast_dir5 = %
go_to_fast_dir6 = ^
go_to_fast_dir7 = &
go_to_fast_dir8 = *
go_to_fast_dir9 = (
go_to_fast_dir10 = )

volume_10 =  M-1
